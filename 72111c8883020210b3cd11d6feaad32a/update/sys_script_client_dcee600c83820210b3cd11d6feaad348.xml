        <applies_extended>false</applies_extended>
        <condition/>
        <description>This script will allow users to input which sort of event will take place if and only if the 'Other' section is selected on the Event Type field.</description>
        <field>u_other_event</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Other Event Input</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   //Type appropriate comment here, and begin script below
	if(newValue == 'other'){
		g_form.setVisible('u_other_event', true);
	}
	else{
		g_form.setVisible('u_other_event', false);
		g_form.setValue('u_other_event', '');
	}
   
}]]></script>
